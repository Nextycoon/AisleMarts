{
  "expo": {
    "name": "AisleMarts",
    "slug": "aislemarts",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aislemarts.app",
      "associatedDomains": [
        "applinks:aislemarts.com",
        "applinks:app.aislemarts.com",
        "applinks:aislemarts.preview.emergentagent.com"
      ],
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "aislemarts-universal-link",
            "CFBundleURLSchemes": ["aislemarts"]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aislemarts.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "aislemarts.com"
            },
            {
              "scheme": "https", 
              "host": "app.aislemarts.com"
            },
            {
              "scheme": "https",
              "host": "aislemarts.preview.emergentagent.com"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "aislemarts"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "scheme": "aislemarts",
    "plugins": [
      "expo-router",
      [
        "expo-linking",
        {
          "scheme": "aislemarts"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}